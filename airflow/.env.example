# Airflow executor
AIRFLOW__CORE__EXECUTOR=CeleryExecutor

# Database (replace with real credentials)
AIRFLOW__DATABASE__SQL_ALCHEMY_CONN=postgresql+psycopg2://airflow:<PASSWORD>@postgres:5432/airflow

# Celery result backend
AIRFLOW__CELERY__RESULT_BACKEND=db+postgresql+psycopg2://airflow:<PASSWORD>@postgres:5432/airflow

# Celery broker
AIRFLOW__CELERY__BROKER_URL=redis://redis:6379/0

# Core settings
AIRFLOW__CORE__LOAD_EXAMPLES=False

# Security (MUST be unique per environment)
AIRFLOW__CORE__FERNET_KEY=<GENERATE_WITH_FERNET>

# Auth manager
AUTH_MANAGER=airflow.providers.fab.auth_manager.fab_auth_manager.FabAuthManager
